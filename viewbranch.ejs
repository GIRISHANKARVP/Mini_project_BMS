<%- layout("boiler")-%>
<div style="margin-top: 10px;">
    <h2 style="text-align: center;color:blueviolet">View Branch Data</h2>
</div>

<div class="container">
<table class="table table bordered">

    <tr> 
        
        <th>BRANCH ID</th>
        <th>BRANCH NAME</th>
        <th>BRANCH CITY</th>
        
        
    </tr>
    <% for( let index=0; index < rows.length; index++ ) 
    { %>
    <tr>
        
        <td>		
            <%=rows[index].bid%>
        </td>
        <td>
            <%=rows[index].bname%>
        </td>
        <td>
            <%=rows[index].bplace%>
        </td>
        
    </tr>
    <% } %>

</table>

<button type="button" ONCLICK="history.back()">BACK</button>
</div>